<Window x:Class="ppedv.ElenasUwe.UI.WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ppedv.ElenasUwe.UI.WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="500">
    <DockPanel>
        <WrapPanel Margin="8" DockPanel.Dock="Top">
            <Button Click="Laden">Laden</Button>
            <Button Click="Delete">Löschen</Button>
            <Button Click="Speichern">Speichern</Button>
        </WrapPanel>
        <DataGrid Name="myGrid">
            <DataGrid.Background>
                <ImageBrush ImageSource="🍷🍷🍷.png"/>
            </DataGrid.Background>
        </DataGrid>
    </DockPanel>
</Window>
